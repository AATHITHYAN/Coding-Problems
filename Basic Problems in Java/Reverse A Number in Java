import java.util.*;

public class Main {

    public static void main(String[] args)
    {
        Scanner sc = new Scanner(System.in);
        
        // write your code here 
        int num = sc.nextInt();
        int temp;
        
        while(num > 0)
        {
            temp = num % 10;
            System.out.println(temp);
            num /= 10;
        }
    }
}
