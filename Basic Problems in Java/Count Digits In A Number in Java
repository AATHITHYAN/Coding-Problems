import java.util.*;
  
  public class Main{
  
  public static void main(String[] args)
  {
    // write your code here 
    Scanner sc = new Scanner(System.in);
    int number = sc.nextInt();
    
    int digits = 0;
    while(number > 0)
    {
        number /= 10;
        digits++;
    }
    
    System.out.println(digits);
   }
  }
